{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.0",
  "description": "Repo Template: vite, vitest, eslint.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 112",
    "ChromeAndroid >= 112",
    "Edge >= 112",
    "Firefox >= 112",
    "FirefoxAndroid >= 112",
    "Safari >= 16.4",
    "iOS >= 16.4",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "preview-host": "vite preview --host",
    "start": "vite --open",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/web-worker": "^0.34.7",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.1",
    "vite": "^5.0.7",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
